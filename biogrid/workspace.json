            "cypressConfig": "apps/biogrid-mvp-e2e/cypress.json",
            "tsConfig": "apps/biogrid-mvp-e2e/tsconfig.e2e.json",
            "devServerTarget": "biogrid-mvp:serve"
          },
          "configurations": {
            "production": {
